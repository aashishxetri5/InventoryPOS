      <div class="page-wrapper">
        <div class="content">
          <div class="page-header">
            <div class="page-title">
              <h4>Item Edit</h4>
              <h6>Update your item</h6>
            </div>
          </div>

          <div class="card">
            <div class="card-body">
              <form
                action="/edititem"
                method="post"
                class="row"
                enctype="multipart/form-data"
              >
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>Item Name</label>
                    <input
                      type="text"
                      name="itemname"
                      value="<%=itemDetail.itemname%>"
                    />
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>Category</label>
                    <select class="select" name="category">
                      <option>Choose Category</option>
                      <option value="Spendable">Spendable</option>
                      <option value="Non-Spendable">Non-Spendable</option>
                    </select>
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>Sub Category</label>
                    <select class="select" name="subCategory">
                      <option>Choose Sub Category</option>
                      <option value="Furniture">Furniture</option>
                      <option value="Stationery">Stationery</option>
                    </select>
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>Brand</label>
                    <select class="select" name="brand">
                      <option>Choose Brand</option>
                      <option value="Asian Pharmaceutical">
                        Asian Pharmaceutical
                      </option>
                      <option value="National Health Care">
                        National Health Care
                      </option>
                      <option value="Arya Pharmaceutical">
                        Arya Pharmaceutical
                      </option>
                      <option value="Index Furniture">Index Furniture</option>
                    </select>
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>Unit</label>
                    <select class="select" name="unit">
                      <option>Choose Unit</option>
                      <option value="piece">Piece</option>
                      <option value="box">Box</option>
                      <option value="set">Set</option>
                      <option value="bottle">bottle</option>
                    </select>
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>SKU</label>
                    <input type="text" name="sku" value="<%=itemDetail.sku%>" />
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>Minimum Qty</label>
                    <input
                      type="text"
                      name="minimumQty"
                      value="<%=itemDetail.minimumQty%>"
                    />
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>Quantity</label>
                    <input
                      type="text"
                      name="quantity"
                      value="<%=itemDetail.quantity%>"
                    />
                  </div>
                </div>
                <div class="col-lg-12">
                  <div class="form-group">
                    <label>Description</label>
                    <textarea class="form-control" name="description">
<%=itemDetail.description%></textarea
                    >
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>Tax</label>
                    <select class="select">
                      <option>Choose Tax</option>
                      <option>2%</option>
                    </select>
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>Discount Type</label>
                    <select class="select">
                      <option>Percentage</option>
                      <option>10%</option>
                      <option>20%</option>
                    </select>
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label>Price</label>
                    <input type="text" value="<%=itemDetail.price%>" />
                  </div>
                </div>
                <div class="col-lg-3 col-sm-6 col-12">
                  <div class="form-group">
                    <label> Status</label>
                    <select class="select">
                      <option>Active</option>
                      <option>Open</option>
                    </select>
                  </div>
                </div>
                <div class="col-lg-12">
                  <div class="form-group">
                    <label> Item Image</label>
                    <div class="image-upload">
                      <input type="file" name="itemImage" />
                      <div class="image-uploads">
                        <img src="/img/icons/upload.svg" alt="img" />
                        <h4>Drag and drop a file to upload</h4>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-12">
                  <div class="item-list">
                    <ul class="row">
                      <li>
                        <div class="itemviews">
                          <div class="itemviewsimg">
                            <img
                              src="<%=itemDetail.itemImage%>"
                              alt="img"
                              width="100px"
                            />
                          </div>
                          <div class="itemviewscontent">
                            <div class="itemviewsname">
                              <h2>
                                <%= itemDetail.itemImage.split('/').pop(); %>
                              </h2>
                              <h3>581kb</h3>
                            </div>
                            <a href="javascript:void(0);" class="hideset">x</a>
                          </div>
                        </div>
                      </li>
                    </ul>
                  </div>
                </div>
                <input
                  type="hidden"
                  value="<%=itemDetail._id%>"
                  name="productId"
                />
                <div class="col-lg-12">
                  <button type="submit" class="btn btn-submit me-2">
                    Update
                  </button>
                  <a href="itemlist" class="btn btn-cancel">Cancel</a>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>

    <script src="/js/jquery-3.6.0.min.js"></script>

    <script src="/js/feather.min.js"></script>

    <script src="/js/jquery.slimscroll.min.js"></script>

    <script src="/js/jquery.dataTables.min.js"></script>
    <script src="/js/dataTables.bootstrap4.min.js"></script>

    <script src="/js/bootstrap.bundle.min.js"></script>

    <script src="/plugins/select2/js/select2.min.js"></script>

    <script src="/plugins/sweetalert/sweetalert2.all.min.js"></script>
    <script src="/plugins/sweetalert/sweetalerts.min.js"></script>

    <script src="/js/script.js"></script>
  </body>
</html>
